<!DOCTYPE html>
<html>
  <head>
    <title>aPoll</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta1/dist/css/bootstrap.min.css" rel="stylesheet">
  </head>
  <body>
    <div class="container">
      <h1 class="mt-3">Polls you have voted in...</h1>
      <div class="text-end">
        <a class="btn btn-primary mt-3" href="/">Home</a>
      </div>
      <hr>

      <% questionNames.forEach(function(e){ %>
        <p>Question name: <%= e.QuestionText %></p>
        <div class="text-end mb-3">
          <a class="btn btn-primary" href="/poll/<%= e.QuestionID %> ">Go to poll</a>
        </div>
        <hr>
      <% }); %> 

    </div>

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta1/dist/js/bootstrap.bundle.min.js"></script>
  </body>
</html>
